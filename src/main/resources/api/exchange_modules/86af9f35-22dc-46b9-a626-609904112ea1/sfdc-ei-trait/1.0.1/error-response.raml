#%RAML 1.0 Trait
responses:
    400:
      description: This response means that server could not understand the request due to invalid syntax.
      body:
        application/json:
          type: !include /exchange_modules/86af9f35-22dc-46b9-a626-609904112ea1/sfdc-ei-data-type/1.0.0/error-response.raml
          example:
            !include /exchange_modules/86af9f35-22dc-46b9-a626-609904112ea1/sfdc-ei-example/1.0.0/error-messages/bad-request-error.raml
    401:
      description: Although the HTTP standard specifies "unauthorized", semantically this response means "unauthenticated". That is, the client must authenticate itself to get the requested response.
      body:
        application/json:
          type: !include /exchange_modules/86af9f35-22dc-46b9-a626-609904112ea1/sfdc-ei-data-type/1.0.0/error-response.raml
          example:
            !include /exchange_modules/86af9f35-22dc-46b9-a626-609904112ea1/sfdc-ei-example/1.0.0/error-messages/invalid-client-error.raml
    405:
      description: The request method is known by the server but has been disabled and cannot be used.
      body:
        application/json:
          type: !include /exchange_modules/86af9f35-22dc-46b9-a626-609904112ea1/sfdc-ei-data-type/1.0.0/error-response.raml
          example:
            !include /exchange_modules/86af9f35-22dc-46b9-a626-609904112ea1/sfdc-ei-example/1.0.0/error-messages/method-not-allowed-error.raml
    500:
      description: The server has encountered a situation it doesn't know how to handle.
      body:
        application/json:
          type: !include /exchange_modules/86af9f35-22dc-46b9-a626-609904112ea1/sfdc-ei-data-type/1.0.0/error-response.raml
          example:
            !include /exchange_modules/86af9f35-22dc-46b9-a626-609904112ea1/sfdc-ei-example/1.0.0/error-messages/internal-server-error.raml
    502:
      description: This error response means that the server, while working as a gateway to get a response needed to handle the request, got an invalid response.
      body:
        application/json:
          type: !include /exchange_modules/86af9f35-22dc-46b9-a626-609904112ea1/sfdc-ei-data-type/1.0.0/error-response.raml
          example:
            !include /exchange_modules/86af9f35-22dc-46b9-a626-609904112ea1/sfdc-ei-example/1.0.0/error-messages/bad-gateway-error.raml
    504:
      description: This error response is given when the server is acting as a gateway and cannot get a response in time.
      body:
        application/json:
          type: !include /exchange_modules/86af9f35-22dc-46b9-a626-609904112ea1/sfdc-ei-data-type/1.0.0/error-response.raml
          example:
            !include /exchange_modules/86af9f35-22dc-46b9-a626-609904112ea1/sfdc-ei-example/1.0.0/error-messages/gateway-timeout-error.raml