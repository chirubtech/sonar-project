#%RAML 1.0 DataType
type: object
properties:
  transactionId:
    type: string
    description: Transaction id generated in API layer
  APIName:
    type: string
    description: Name of the invoked API
  status:
    type: integer
    description: HTTP status code
  message:
    type: string
    description: Summary of error
  timeStamp:
    type: string
    description: Time wehen the event occured
  errorType:
    type: string
    description: Type of error