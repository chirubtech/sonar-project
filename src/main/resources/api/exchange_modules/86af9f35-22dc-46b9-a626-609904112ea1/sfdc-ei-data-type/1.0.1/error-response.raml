#%RAML 1.0 DataType
type: object
properties:
  transactionId:
    required: false
    type: string
    description: Transaction id generated in API layer
  APIName:
    required: false
    type: string
    description: Name of the invoked API
  message:
    required: false
    type: string
    description: Summary of error
  cause:
    required: false
    type: string
    description: Type of error